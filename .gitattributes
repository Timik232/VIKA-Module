# Auto detect text files and perform LF normalization
* text=auto
*.pckl filter=lfs diff=lfs merge=lfs -text
model.pkl filter=lfs diff=lfs merge=lfs -text
vector.pkl filter=lfs diff=lfs merge=lfs -text
dictionary.pickle filter=lfs diff=lfs merge=lfs -text
mirea_users.pickle filter=lfs diff=lfs merge=lfs -text
